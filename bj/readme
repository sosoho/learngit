#awk unix2dos dos2unix
awk 'NR>1 {print $0}' read_file >> write_file
awk 'BEGIN{FS="\t";OFS=","} {print $1,$3,$4}' file_read # default is stdout
dos2unix file1 file2 file3... filen # operater on itself
dos2unix -n oldfile newfile
dos2unix -k file1 # remain the timestamp

